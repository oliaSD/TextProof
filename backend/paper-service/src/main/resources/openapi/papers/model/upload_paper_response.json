{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Ответ на загрузку статьи пользователя в системе",
  "properties": {
    "username": {
      "type": "string",
      "title": "Имя пользователя",
      "example": "olga"
    },
    "fileMetadata": {
      "description": "Метаданные файла",
      "$ref": "file_metadata.json"
    }
  },
  "required": [
    "username",
    "fileMetadata"
  ]
}
