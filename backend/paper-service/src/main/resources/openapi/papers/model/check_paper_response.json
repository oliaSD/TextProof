{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Ответ на получение информации о пользователе в системе",
  "properties": {
    "status": {
      "description": "Статус проверки",
      "type": "string",
      "enum": [
        "check",
        "non_check",
        "finish_check"
      ]
    },
    "reportId": {
      "description": "Номер отчета",
      "type": "integer",
      "format": "int64",
      "example": "1234"
    }
  },
  "required": [
    "status",
    "reportId"
  ]
}
